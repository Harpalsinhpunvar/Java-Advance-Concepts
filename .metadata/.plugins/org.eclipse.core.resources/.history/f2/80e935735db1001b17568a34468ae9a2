package com.edureka;

interface cab3{
	void bookedcab(String msg);
}

interface cab4{
	double bookedcab4(String msg, String msg2);
}
public class Lambda_Expression {

	public static void main(String[] args) {
		
		cab3 obj= (msg)->System.out.println("Booked: " + msg);
		obj.bookedcab("Arriving soon!");
		
		cab4 obj2 = (msg, msg2)-> {System.out.println("booked" + msg + " to " + msg2); return 980.10;}
		obj2.bookedcab4("pune", "mumbai");
		
		

	}

}
